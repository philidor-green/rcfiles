{
    "workbench.colorCustomizations": {
        "sash.hoverBorder": "#e4381aad",
        "editorWarning.background": "#80fbff28",
        "editorWarning.foreground": "#e4381aad",
        "editorError.background": "#ff80ab66",
        "editorError.foreground": "#d50000",
        "editor.findMatchBackground": "#9f40ff2b",
        "editor.findMatchBorder": "#9f40ff2b",
        "editor.findMatchHighlightBackground": "#d1f825cc",
        "editor.findMatchHighlightBorder": "#383838cc",
        "editor.findRangeHighlightBackground": "#9f40ff40",
        "editorWhitespace.foreground": "#c1c6d3d0", // theme background
        "editorIndentGuide.background": "#babdc5ad" // indent guide color
    },
    // "editor.fontFamily": "'Iosevka Term SS12', Consolas, 'Courier New', monospace",
    "editor.fontFamily": "'Monofur Nerd Font Mono', Consolas, 'Courier New', monospace",
    "editor.fontWeight": 300,
    "markdown.preview.fontSize": 13,
    "markdown.preview.fontFamily": "'Monofur Nerd Font Mono', Consolas, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.cursorSmoothCaretAnimation": false,
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.foldfix": true,
    // "workbench.colorCustomizations": {
    //     "statusBar.background": "#f79a36",
    //     "statusBar.foreground": "#3d1010",
    //     "activityBar.background": "#000e33",
    //     "activityBar.foreground": "#404040"
    // },
    "breadcrumbs.enabled": true,
    "vim.whichwrap": "h,l,<,>,[,]",
    "vim.easymotion": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        },
        {
            "before": [
                "Z",
                "Q"
            ],
            "commands": [
                ":q!"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "notifications.clearAll"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.new"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "workbench.startupEditor": "newUntitledFile",
    // "editor.quickSuggestions": {
    //     "comments": "off",
    //     "strings": "off",
    //     "other": "off"
    // },
    "go.formatTool": "goimports",
    "explorer.confirmDelete": false,
    "terminal.integrated.showExitAlert": false,
    "terminal.external.linuxExec": "foot",
    "terminal.integrated.cursorBlinking": true,
    "git.enableSmartCommit": true,
    "workbench.activityBar.visible": true,
    "editor.fontLigatures": true,
    "go.coverOnTestPackage": false,
    "go.gopath": "/Users/think/.go",
    "go.lintFlags": [
        "--config=${workspaceFolder}/.golangci.yml"
    ],
    "[go]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "go.lintTool": "golangci-lint",
    "editor.lightbulb.enabled": false,
    // "editor.parameterHints.enabled": false,
    "editor.hover.enabled": true,
    "editor.hover.sticky": false,
    "editor.renderWhitespace": "boundary",
    "editor.useTabStops": true,
    "editor.renderControlCharacters": true,
    "terminal.integrated.fontSize": 15,
    "window.dialogStyle": "custom",
    "terminal.integrated.fontWeight": 500,
    "terminal.integrated.fontFamily": "'Monofur Nerd Font Mono', Consolas, 'Courier New', monospace",
    // "terminal.integrated.fontFamily": "'Inconsolata Expanded Light', monospace",
    "go.testFlags": [
        "-v"
    ],
    "gopls": {
        "analyses": {
            "copylocks": false,
            "composites": false,
            "simplifycompositelit": false
        }
    },
    "solidity.packageDefaultDependenciesContractsDirectory": "",
    "solidity.packageDefaultDependenciesDirectory": "node_modules",
    "solidity.compileUsingRemoteVersion": "v0.6.6",
    "solidity.formatter": "none",
    "terminal.integrated.detectLocale": "off",
    "solidity.linter": "solium",
    "files.autoSave": "afterDelay",
    "explorer.confirmDragAndDrop": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "window.menuBarVisibility": "compact",
    "go.vetOnSave": "off",
    "go.toolsManagement.autoUpdate": true,
    "vetur.format.enable": true,
    "[vue]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Vue.volar"
    },
    "eslint.format.enable": true,
    // "vetur.useWorkspaceDependencies": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "html",
        "vue"
    ],
    "eslint.options": {
        "extensions": [
            ".html",
            ".js",
            ".vue",
            ".jsx"
        ]
    },
    "eslint.onIgnoredFiles": "warn",
    "eslint.workingDirectories": [
        "./",
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": false,
    "vetur.format.defaultFormatter.js": "none",
    "vetur.format.defaultFormatter.html": "none",
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
    "vetur.validation.template": true,
    "vetur.format.defaultFormatter.css": "none",
    "vetur.format.defaultFormatter.less": "none",
    "vetur.format.defaultFormatter.postcss": "none",
    "vetur.format.defaultFormatter.pug": "none",
    "vetur.format.defaultFormatter.sass": "none",
    "vetur.format.defaultFormatter.scss": "none",
    "vetur.format.defaultFormatter.stylus": "none",
    "vetur.format.defaultFormatter.ts": "none",
    "vetur.languageFeatures.codeActions": false,
    "vetur.languageFeatures.updateImportOnFileMove": false,
    "eslint.debug": false,
    "eslint.packageManager": "yarn",
    "eslint.run": "onSave",
    "html.format.enable": false,
    "remote.autoForwardPorts": false,
    "remoteHub.autoFetch.enabled": false,
    "projectManager.sortList": "Name",
    "projectManager.git.baseFolders": [
        ".git"
    ],
    "workbench.sash.size": 2,
    "docker.images.sortBy": "CreatedTime",
    "rest-client.enableTelemetry": false,
    "rest-client.fontFamily": "'Anka/Coder'",
    "rest-client.fontSize": 10,
    "rest-client.previewColumn": "current",
    "rest-client.fontWeight": "300",
    "go.survey.prompt": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "files.legacyWatcher": "on",
    "files.watcherExclude": {
        "**/mongo/*": true
    },
    "workbench.colorTheme": "GitHub Light Colorblind (Beta)",
    "[rust]": {
        "editor.formatOnSave": true
    },
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "css.lint.unknownAtRules": "ignore",
    "css.validate": true,
    "scss.validate": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "crates.compatibleDecorator": "${version}"
}
